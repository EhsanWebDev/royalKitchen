{
  "expo": {
    "name": "RoyalKitchen",
    "slug": "RoyalKitchen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": true,

      "bundleIdentifier": "com.ehsanwebdev47.RoyalKitchen",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.706778063952-u6kf1vcilqa8ens89pork3eo6d66bj74"
        }
      },

      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.ehsanwebdev47.RoyalKitchen",
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
